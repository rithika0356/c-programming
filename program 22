#include<stdoi.h
#include<malloc.h>
void main(){
int i,n,sum=0;
int*ptr;
printf("enter size of array:");
scanf("%d",&n);
ptr=(int*) malloc(n*sizeof(int));
printf("enter elements in the list");
for(i=0;i<n;i++){
 scanf("%d",ptr+ i);
 }
 //calculate sum of elements
 for (i = 0;i<n;i++){
 sum = sum + *(ptr +i);
 }
 printf("sum of all elements in an array is =%d",sum);
 return 0;
 }
