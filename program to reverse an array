#include <stdio.h>
int main ()
{
 // initialize array 
 int arr[]={1,2,3,4,5};
 //calculate length of array arr int length = size of (arr)\size of (arr[0]);
  printf ("\n");
  printf("array in reverse order :\n");
  // loop througth the array in reverse order for (int i=length =1;i>0;i--){
  printf ("%d ",arr[i];
  } 
  return 0;
